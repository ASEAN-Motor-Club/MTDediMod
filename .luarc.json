{
    "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
    "runtime": {
        "version": "Lua 5.4"
    },
    "workspace": {
        "maxPreload": 50000,
        "preloadFileSize": 5000,
        "library": [
            "types/ue4ss",
            "types/game",
            "types/luasocket"
        ],
        "checkThirdParty": false,
        "ignoreDir": [
            "types/game"
        ]
    },
    "diagnostics": {
        "disable": [
            "lowercase-global"
        ],
        "severity": {
            "undefined-doc-name": "Hint",
            "local-limit": "Hint",
            "inject-field": "Hint",
            "miss-name": "Hint"
        },
        "groupSeverity": {
            "types/game/*": "Hint"
        }
    },
    "hint": {
        "enable": true,
        "paramName": "All",
        "setType": true
    }
}
